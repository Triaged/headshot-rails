<div class="about-hero">
    <div class="about-background">
        <div class="about-row">
            <div class="container">
                <div class="row">
                    <div class="section-header about-header col-lg-10 col-lg-offset-1 col-xs-12">
                        <div class="divider-line divider-about">
                            <span>
                                <small>Our Team</small>
                            </span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-2 col-lg-offset-1">
                    <%= image_tag_with_at2x ("team/charlie.png"), :class => "about-img"  %>
                </div>
                <div class="col-lg-2 about-column">
                    <%= image_tag_with_at2x ("team/nick.png"), :class => "about-img"  %>
                </div>
                <div class="col-lg-2 about-column">
                    <%= image_tag_with_at2x ("team/jeff.png"), :class => "about-img" %>
                </div>
                <div class="col-lg-2 about-column">
                    <%= image_tag_with_at2x ("team/alex.png"), :class => "about-img"  %>
                </div>
                <div class="col-lg-2 about-column">
                    <%= image_tag_with_at2x ("team/jt.png"), :class => "about-img"  %>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="content container">
    <div class="investor-group">
            <div class="row">
                <div class="section-header col-lg-10 col-lg-offset-1 col-xs-12">
                    <div class="divider-line">
                        <span>
                            <small>Our Investors</small>
                        </span>
                    </div>
                </div>
            </div>
            <div class="investor-logos row">
                <div class="col-xs-12 col-lg-10 col-lg-offset-1">
                    <div class="row">
                        <div class="col-xs-12 col-lg-2 col-lg-offset-1">
                            <a href="http://www.vastvc.com/"><%= image_tag ("logos/vast.png") %></a>
                        </div>
                        <div class="col-lg-2 col-xs-12">
                            <a href="http://www.flybridge.com/"><%= image_tag ("logos/flybridge.png") %></a>
                        </div>
                        <div class="col-lg-2 col-xs-12">
                            <a href="http://www.lererventures.com/"><%= image_tag ("logos/lererventures.png") %></a>
                        </div>
                        <div class="col-lg-2 col-xs-12">
                            <a href="http://www.bloombergbeta.com/"><%= image_tag ("logos/bloombergbeta.jpg") %></a>
                        </div>
                        <div class="col-lg-2 col-xs-12">
                            <a href="http://mesa.vc/"><%= image_tag ("logos/mesa.png") %></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</div>
