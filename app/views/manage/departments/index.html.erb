<h1>Listing manage_departments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manage_departments.each do |manage_department| %>
      <tr>
        <td><%= link_to 'Show', manage_department %></td>
        <td><%= link_to 'Edit', edit_manage_department_path(manage_department) %></td>
        <td><%= link_to 'Destroy', manage_department, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Department', new_manage_department_path %>
