<div class="contact-breadcrumb border">
  <h2>
  <%= link_to 'Contacts', manage_users_path %> <span class="breadcrumb-fwdslsh">/</span> Import Contacts
  </h2>
</div>

<div class="row">
  <div class="col-lg-6">

    <div class="profile">
      <div class="profile-picture">
        <div class="icon-circle">
          <div class="icon">
            <%= image_tag "admin/google-apps-logo.png" %>
          </div>
        </div>
      </div>
      <div class="profile-info">
        <div class="profile-name">
          <h2>Google Apps</h2>
        </div>
        <div class="profile-email">
          <p>Connect your Google App account to import your contacts.</p>
        </div>
      </div>
      <div class="btns-center btns-spacing-m">
        <%= link_to "Import from Google Apps", user_omniauth_authorize_url(:google_oauth2, subdomain: nil, state: current_company.name, :protocol => 'https'), class: "btn-custom btn-primary" %>
      </div>
    </div>
  </div>

  <div class="col-lg-6">

    <div class="profile">
      <div class="profile-picture">
        <div class="icon-circle">
          <div class="icon">
            <%= image_tag "admin/BambooHR-logo.png" %>
          </div>
        </div>
      </div>
      <div class="profile-info">
        <div class="profile-name">
          <h2>BambooHR</h2>
        </div>
        <div class="profile-email">
          <p>Connect your BambooHR account to import your contacts.</p>
        </div>
      </div>
      <div class="btns-center btns-spacing-m">
        <%= link_to "Import from BambooHR", bamboohr_manage_import_index_path, class: "btn-custom btn-primary" %>
      </div>
    </div>
  </div>
</div>
